{
  "config": {
	"vendor-dir": "inc/",
	"allow-plugins": {
      "phpstan/extension-installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "name": "eight20results/pmpro-import-members-from-csv",
  "type": "plugin",
  "description": "Import and create user + PMPro member records from a CSV file to your WordPress with Paid Memberships Pro website. Can import the membership information, user meta data, PMPro order data and will link any pre-existing recurring payment records for your payment gateway integration.",
  "keywords": ["wordpress",
	  "paid memberships pro",
	  "pmpro",
	  "import",
	  "csv",
	  "member import",
	  "user import",
	  "buddypress import",
	  "buddypress"
  ],
  "minimum-stability": "stable",
  "homepage": "https://eighty20results.com",
  "license": "GPLv2",
  "authors": [
  {
	"name": "Thomas Sjolshagen",
	"email": "thomas@eighty20results.com",
	"homepage": "https://eighty20results.com/thomas-sjolshagen",
	"role": "Developer"
  }
  ],
  "require": {
	"php": ">=7.1",
	"10quality/license-keys-php-client": ">=v1.2.2",
	"mixpanel/mixpanel-php" : "2.*",
	"ext-curl": "*",
	"ext-json": "*"
  },
  "require-dev": {
	"10up/wp_mock": "^0.4.2",
	"squizlabs/php_codesniffer": ">=3.5.8",
	"automattic/vipwpcs": "^2.3.0",
	"wp-coding-standards/wpcs": "^2.3.0",
	"automattic/phpcs-neutron-standard": "^v1.7.0",
	"phpstan/phpstan": "^0.12.78",
	"phpmetrics/phpmetrics": ">=v1.10.0",
	"phpunit/phpunit": "^9.5",
	"codeception/codeception": "^4.1.0",
	"codeception/module-asserts": "^1.3",
	"codeception/module-phpbrowser": "^1.0",
	"codeception/module-webdriver": "=1.2.1",
	"codeception/module-db": "^1.1",
	"codeception/module-filesystem": "^1.0",
	"codeception/module-cli": "^1.1.0",
	"codeception/util-universalframework": "^1.0.0",
	"codeception/aspect-mock": "<4.0",
	"codeception/assert-throws": "^1.1.0",
	"codeception/module-rest": "^1.3",
	"spatie/phpunit-snapshot-assertions": "^4.2.5",
	"lucatume/wp-browser": "^3.0",
	"brain/monkey": "=2.6.1",
	"fakerphp/faker": "^v1.20",
	"szepeviktor/phpstan-wordpress": "^v0.7",
	"mikey179/vfsstream": "~1",
	"phpstan/extension-installer": "^1.1",
	"php-mock/php-mock": "2.3.0",
	"ext-mysqli": "*"
  },
  "scripts": {
	"selenium-start": "selenium-server -port 4444",
	"post-install-cmd": "if [ -f inc/bin/phpcs ] ; then inc/bin/phpcs --config-set installed_paths inc/automattic/phpcs-neutron-standard/NeutronStandard,inc/wp-coding-standards/wpcs,inc/automattic/vipwpcs ; fi",
	"post-update-cmd" : "if [ -f inc/bin/phpcs ] ; then inc/bin/phpcs --config-set installed_paths inc/automattic/phpcs-neutron-standard/NeutronStandard,inc/wp-coding-standards/wpcs,inc/automattic/vipwpcs ; fi"
  },
  "autoload": {
	"psr-4": {
	  "ProjectRoot\\" : "",
	  "NamespaceInSourceDir\\": "",
	  "E20R\\": "src/E20R",
	  "E20R\\Utilities\\": "",
	  "E20R\\Import\\": "./",
	  "E20R\\Import_Members\\": "src/E20R/Import_Members",
	  "E20R\\Import_Members\\Email\\": "src/E20R/Import_Members/Email",
	  "E20R\\Import_Members\\Process\\": "src/E20R/Import_Members/Process",
	  "E20R\\Import_Members\\Modules\\": "src/E20R/Import_Members/Modules",
	  "E20R\\Import_Members\\Modules\\BuddyPress\\": "src/E20R/Import_Members/Modules/BuddyPress",
	  "E20R\\Import_Members\\Modules\\PMPro\\": "src/E20R/Import_Members/Modules/PMPro",
	  "E20R\\Import_Members\\Modules\\Users\\": "src/E20R/Import_Members/Modules/Users",
	  "E20R\\Import_Members\\Validate\\": "src/E20R/Import_Members/Validate",
	  "E20R\\Import_Members\\Validate\\Column_Values\\": "src/E20R/Import_Members/Validate/Column_Values"
	}
  },
  "autoload-dev": {
	  "psr-4": {
		  "ProjectRoot\\" : "",
		  "NamespaceInSourceDir\\": "",
		  "E20R\\": "src/E20R",
		  "E20R\\Tests\\": "tests",
		  "E20R\\Tests\\Fixtures\\Factory\\": "tests/_fixtures/factory",
		  "E20R\\Tests\\Unit\\": "tests/unit/testcases",
		  "E20R\\Tests\\Integration\\": "tests/integration/testcases",
		  "E20R\\Tests\\Functional\\": "tests/functional/testcases",
		  "E20R\\Tests\\Acceptance\\": "tests/acceptance/testcases",
		  "E20R\\Tests\\API\\": "tests/api/testcases",
		  "E20R\\Tests\\Unit\\Fixtures\\": "tests/unit/inc",
		  "E20R\\Tests\\Integration\\Fixtures\\": "tests/integration/inc"
	  }
  }
}
